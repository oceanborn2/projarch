<specfile>
 <spec name="DocumentStatus" datamodel="value_domain" datatype="INT" prefix="DOC_STATUS_">
  <entry name="initialized" value="1" comment="Is the document initialised ?"/>
  <entry name="draft" value="2" comment="Is the document a draft ?"/>
  <entry name="released" value="3" comment="Is the document released ?"/>
  <entry name="obsolete" value="4" comment="Is the document obsolete ?"/> 
 </spec>
</specfile>

