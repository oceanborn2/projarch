<specfile>

<spec name="Batches" datamodel="busobj">
	<entry name="id" datatype="sequence"/>
	<entry name="name"  datatype="string" nullable="yes" size="40"/>
	<entry name="description"  datatype="string" nullable="yes" size="80"/>
	<entry name="estimated_start_date"  datatype="date" nullable="no"/>
	<entry name="estimated_end_date"  datatype="date" nullable="no"/>
	<entry name="actual_start_date"  datatype="date" nullable="no"/>
	<entry name="actual_end_date"  datatype="date" nullable="no"/>
	<entry name="duration" datatype="duration" nullable="yes"/>
	<entry name="cost1" datatype="numeric" nullable="yes"/>
	<entry name="cost2" datatype="numeric" nullable="yes"/>
	<entry name="requirements" datatype="collection" nullable="yes"/>
	<entry name="folders "     datatype="collection" nullable="yes"/>
	<entry name="documents"    datatype="collection" nullable="yes"/>
	<entry name="keywords"     datatype="collection" nullable="yes"/> 
	<entry name="priority"     datatype="numeric"    nullable="yes"/>
	<entry name="related_release" datatype="numeric" nullable="yes"/>
</spec>

</specfile>











