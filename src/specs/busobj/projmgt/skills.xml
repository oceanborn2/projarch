<specfile>

<spec name="Skills" datamodel="busobj">
	<entry name="id" datatype="sequence"/>
	<entry name="description" datatype="string" nullable="yes" size="40"/>
	<entry name="skilldatatype"   datatype="value_domain" composed="value_domains.skilldatatypes" mincard="1" maxcard="1" nullable="yes"/>
	<entry name="skilllevel"  datatype="value_domain" composed="value_domains.skilllevel" mincard="1" maxcard="1" nullable="yes"/>
	<entry name="comment"     datatype="memo" nullable="yes"/>
</spec>

</specfile>











