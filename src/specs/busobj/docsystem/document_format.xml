<spec name="DocumentFormat"    datamodel="busobj">
	<entry name="id"           datatype="sequence" nullable="no"/>
	<entry name="ident"        datatype="string"       length="20"              nullable="no"/>
	<entry name="name"         datatype="string"       length="40"              nullable="yes"/>
